sudo forever-service delete poolapi
sudo forever-service install poolapi -s init.js -o " -module=api" -e "emailAddressFrom=$emailAddressFrom rustySessionSecret=$rustySessionSecret rustyReCaptchaKey=$rustyReCaptchaKey emailPassword=$emailPassword emailServer=$emailServer emailUser=$emailUser rustyDbUrl=$rustyDbUrl"
sudo forever-service delete poolweb
