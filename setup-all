sudo forever-service delete poolcore
sudo forever-service install poolcore -s init.js -o " -module=pool" -e "emailAddressFrom=$emailAddressFrom rustySessionSecret=$rustySessionSecret rustyReCaptchaKey=$rustyReCaptchaKey emailPassword=$emailPassword emailServer=$emailServer emailUser=$emailUser rustyDbUrl=$rustyDbUrl"
sudo forever-service delete poolpay
sudo forever-service install poolpay -s init.js -o " -module=payments" -e "emailAddressFrom=$emailAddressFrom rustySessionSecret=$rustySessionSecret rustyReCaptchaKey=$rustyReCaptchaKey emailPassword=$emailPassword emailServer=$emailServer emailUser=$emailUser rustyDbUrl=$rustyDbUrl"
sudo forever-service delete poolunlocker
